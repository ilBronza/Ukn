{
    "name": "ilbronza/ukn",
    "description": "uikit flash notifications",
    "license": "MIT",
    "authors": [
        {
            "name": "Davide Bronzini",
            "email": "davide@sistema.srl",
            "homepage": "https://www.sistema.srl"
        }
    ],
    "homepage": "https://github.com/ilbronza/ukn",
    "keywords": ["Laravel", "Ukn"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "IlBronza\\Ukn\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IlBronza\\Ukn\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IlBronza\\Ukn\\UknServiceProvider"
            ],
            "aliases": {
                "Ukn": "IlBronza\\Ukn\\Facades\\Ukn"
            }
        }
    }
}
